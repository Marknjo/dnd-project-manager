#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/esbuild@0.17.12/node_modules/esbuild/bin/node_modules:/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/esbuild@0.17.12/node_modules/esbuild/node_modules:/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/esbuild@0.17.12/node_modules:/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/esbuild@0.17.12/node_modules/esbuild/bin/node_modules:/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/esbuild@0.17.12/node_modules/esbuild/node_modules:/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/esbuild@0.17.12/node_modules:/home/mark_empire/local-dev/projects/dnd-project-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../../../../../esbuild@0.17.12/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
